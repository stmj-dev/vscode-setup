	"anonymous function": {
		"prefix": "anofunc",
		"body": ["function (${1:arguments}) {", "\t${2:// body}", "}"],
		"description": "anonymous function"
	},
	"arrow function": {
		"prefix": "infunc",
		"body": ["fn (${1:arguments}) => ${2:code}"],
		"description": "Arrow function"
